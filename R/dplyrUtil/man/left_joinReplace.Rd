## The function is currently defined as
function (x, y, by, ...) 
{
    addCols <- setdiff(names(y), by)
    delCols <- intersect(names(x), addCols)
    xD <- if (length(delCols)) 
        select(x, -one_of(delCols))
    else x
    left_join(xD, y, by = by, ...)
  }
}
